{
  "name": "use-grid",
  "homepage": "/",
  "version": "0.8.1",
  "module": "index.js",
  "types": "index.d.ts",
  "main": "index.cjs.js",
  "author": "tsei",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "description": "ðŸ¤ use-grid is a hook to build responsive layouts of all shapes and sizes.",
  "repository": "https://github.com/tseijp/use-grid",
  "bugs": "https://github.com/tseijp/use-grid/issues",
  "keywords": [
    "react",
    "typescript",
    "material-ui",
    "material design",
    "bootstrap",
    "grid"
  ],
  "dependencies": {
    "react": "^16.13.1"
  },
  "scripts": {
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "start": "cross-env NODE_ENV=development BABEL_ENV=development react-app-rewired start",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production react-app-rewired build",
    "compile": "cross-env NODE_ENV=production BABEL_ENV=production rollup -c config-rollup.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@tsei/core": "^0.3.3",
    "@tsei/mdmd": "^2.0.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.48",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "cross-env": "^7.0.2",
    "fs": "0.0.1-security",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-spring": "^9.0.0-rc.3",
    "react-syntax-highlighter": "^12.2.1",
    "react-use-gesture": "^7.0.15",
    "rollup": "^2.21.0",
    "typescript": "^3.7.5",
    "use-grid": "^0.6.0"
    },
    "jest": {
        "collectCoverageFrom": [
          "src/**/*.{js,jsx,ts,tsx}",
          "!src/**/*.d.ts",
          "!src/index.{ts,tsx}",
          "!src/serviceWorker.ts"
        ],
        "coveragePathIgnorePatterns": [
          "./src/*/*.types.{ts,tsx}",
          "./src/index.tsx",
          "./src/serviceWorker.ts"
        ],
        "coverageReporters": [
          "json",
          "lcov",
          "text-summary",
          "clover"
        ],
        "coverageThreshold": {
          "global": {
            "statements": 95,
            "branches": 95,
            "lines": 95,
            "functions": 95
          }
        },
        "snapshotSerializers": [
          "enzyme-to-json/serializer"
        ],
        "transform": {
          "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
        },
        "transformIgnorePatterns": [
          "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
          "^.+\\.module\\.(css|sass|scss)$"
        ],
        "moduleNameMapper": {
          "^react-native$": "react-native-web",
          "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
          "src/(.*)$": "<rootDir>/src/$1"
        }
    }
}
